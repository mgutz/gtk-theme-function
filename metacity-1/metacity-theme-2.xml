<draw_ops name="outline">
    <include name="bg_borders"/>

    <!-- TOP LINE -->
    <line color="#AAAAAA" width="1" x1="0" y1="0" x2="width" y2="0"/>

    
    <!-- ROUNDING ANTIALIAS -->
    <!-- top-left arc hilite -->
    <arc color="shade/#AAAAAA/0.98" from="270.0" to="360.0" filled="false"  x="0" y="0" width="8" height="8"/>
    <!-- top-right arc hilite -->
    <arc color="shade/#AAAAAA/0.98" from="0.0" to="90.0" filled="false"  x="width-9" y="0" width="8" height="8"/>
    

    <!-- LEFT LINE -->
    <!-- topbar -->
    <gradient type="vertical" x="0" y="0" width="1" height="(title_height + 8)">
    <color value="#AAAAAA"/>
    <color value="#8a8a8a"/>
    </gradient>
    <!-- client area -->
    <gradient type="vertical" x="0" y="(title_height + 8)" width="1" height="height - (title_height + 8)">
    <color value="#8a8a8a"/>
    <color value="#606060"/>
    </gradient>

    <!-- RIGHT LINE -->
    <!-- topbar -->
    <gradient type="vertical" x="width-1" y="0" width="1" height="(title_height + 8)">
    <color value="#AAAAAA"/>
    <color value="#8a8a8a"/>
    </gradient>
    <!-- client area -->
    <gradient type="vertical" x="width-1" y="(title_height + 8)" width="1" height="height - (title_height + 8)">
        <color value="#8a8a8a"/>
        <color value="#606060"/>
    </gradient>

    <!-- bottom line -->
    <line color="#606060" width="1" x1="0" y1="height - 1" x2="width" y2="height - 1"/>

    <!-- topbar shadow -->
    <line color="#505050" width="1" x1="0" y1="(title_height + 8) -1" x2="width" y2="(title_height + 8) -1"/>
</draw_ops>

